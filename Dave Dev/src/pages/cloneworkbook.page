<apex:page standardController="Workbook__c" extensions="workbookExtension">
  <apex:sectionHeader title="Clone Workbook"/>
  <apex:form >
      <apex:pageblock >
          <apex:pageBlockButtons >
              <apex:commandButton value="Cancel" action="{!cancel}"/>
              <apex:commandButton value="Submit" action="{!submit}"/>
          </apex:pageBlockButtons>
          <apex:pageBlockSection >
              <apex:pageBlockSectionItem >
                  <apex:outputLabel value="Select Account to attach cloned workbook to"/>
                  <apex:inputField value="{!newwb.Account__c}"/>    
              </apex:pageBlockSectionItem>
          </apex:pageBlockSection>
      </apex:pageblock>
  </apex:form>
</apex:page>